  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch index.html",
      "type": "chrome",
      "request": "launch",
      "reAttach": true,
      "url": "http://localhost:3000/App",
      "webRoot": "${workspaceFolder}",
      "pathMappings": [
        {
          "url": "http://localhost:3000/App",
          "path": "${workspaceFolder}"
        }
      ]
    }
  ]
}
